
public class Solution {
    
    public static void swapAlternate(int arr[]) {
    	//Your code goes here
        int n=arr.length;
        int count=0;
        if(n%2!=0)
        {
            count++;
        }    
        	for(int i=0;i<n-count;i=i+2)
        	{
            	
                	int temp=arr[i];
                	arr[i]=arr[i+1];
                	arr[i+1]=temp;
                	continue;
            	
        	}

    }
}
