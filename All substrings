
public class Solution {

	public static void printSubstrings(String str) {
		//Your code goes here
        int n=str.length();
        for(int i=0;i<n;i++)
        {
            for(int j=i;j<n;j++)
            {
                String substr=str.substring(i,j+1);
                System.out.println(substr);
                
            }
           
        }
        
	}

}
