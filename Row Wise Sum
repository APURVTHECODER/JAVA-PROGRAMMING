
public class Solution {

	public static void rowWiseSum(int[][] mat) {
		//Your code goes here
        if(mat.length==0 || mat[0].length==0)
        {
            return;
        }
        int n=mat.length;
        int m=mat[0].length;
        for(int i=0;i<n;i++)
        {
            int sum=0;
            for(int j=0;j<m;j++)
            {
               sum+=mat[i][j];
                
            }
            System.out.print(sum+" ");
        }
	}

}
