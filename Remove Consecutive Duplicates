
public class Solution {

    public static String removeConsecutiveDuplicates(String s) {

        String ans = "";
    //   ans += s.charAt(0);
      for(int i = 0; i<s.length()-1; i++)
      {
          if(s.charAt(i) == s.charAt(i+1))
          {
              continue;
          }
          else{
              ans += s.charAt(i);
          }
      }
      ans+=s.charAt(s.length()-1);
      return ans;

    }

}
