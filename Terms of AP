import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
    Scanner s = new Scanner(System.in);
    int n=s.nextInt();
    int i=1;
    int count=0;
    for(i=1;i<=n;i++)
    {   
        for(int j=3*i+2;i<=n+count;i++)
        {
            if((3*i+2)%4!=0)
                System.out.print(3*i+2 + " ");
            else
                count++;
        }
    }

    
	}
}
