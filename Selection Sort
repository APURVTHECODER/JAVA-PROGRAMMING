
public class Solution {  

    public static void selectionSort(int[] arr) {
    	//Your code goes here
    int n=arr.length;
    for(int i=0;i<n-1;i++)
    {
        for(int j=i;j<n;j++)
        {
            if(arr[i]>arr[j])
            {
                int temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
                continue;
            }
        }
    }
        
        
        
        
    }   

}
