import java.util.Scanner;
public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        
            Scanner s = new Scanner(System.in);


    String str = s.next();
    char ch=str.charAt(0);
    int x=(int)ch;
    if(x>=97 && x<=122)
    {
        System.out.println("0");
    }
    if(x>=65 && x<=90)
    {
        System.out.println("1");
        
    }
    if(x>=33 && x<=57)
    {
        System.out.println("-1");
    }

    }
}
