public class Solution {  

    public static void pushZerosAtEnd(int[] arr) {
    	//Your code goes here
    int n=arr.length;
    int i=0;
    int k=0;
    while(i < n && k < n)
    {
        
        if(arr[i]!=0)
        {
            int temp=arr[i];
            arr[i]=arr[k];
            arr[k]=temp;
            i++;
            k++;
        }
        else
        {
            i++;
        }
        
        
        
    }
             




    }
        

    
}

